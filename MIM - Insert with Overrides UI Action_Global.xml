on&gt;&lt;list_banner_button&gt;false&lt;/list_banner_button&gt;&lt;list_button&gt;false&lt;/list_button&gt;&lt;list_choice&gt;false&lt;/list_choice&gt;&lt;list_context_menu&gt;false&lt;/list_context_menu&gt;&lt;list_link&gt;false&lt;/list_link&gt;&lt;list_save_with_form_button&gt;false&lt;/list_save_with_form_button&gt;&lt;list_style/&gt;&lt;name&gt;Insert with Overrides&lt;/name&gt;&lt;onclick/&gt;&lt;order&gt;400&lt;/order&gt;&lt;script&gt;&lt;![CDATA[doInsertWithOverrides();

function doInsertWithOverrides() {
  var oldDefinition = current.sys_id.toString();
  var saveMe = current;
  var newDefinition = current.insert();
  var overrides = new GlideRecord('contact_default_override');
  overrides.addQuery('parent',oldDefinition);
  overrides.query();
  while (overrides.next()) {
    overrides.parent = newDefinition;
    overrides.insert();
  }
  action.setRedirectURL(saveMe);
}]]&gt;&lt;/script&gt;&lt;show_insert&gt;false&lt;/show_insert&gt;&lt;show_multiple_update&gt;false&lt;/show_multiple_update&gt;&lt;show_query&gt;false&lt;/show_query&gt;&lt;show_update&gt;true&lt;/show_update&gt;&lt;sys_class_name&gt;sys_ui_action&lt;/sys_class_name&gt;&lt;sys_created_by&gt;admin&lt;/sys_created_by&gt;&lt;sys_created_on&gt;2018-09-18 11:49:53&lt;/sys_created_on&gt;&lt;sys_domain&gt;global&lt;/sys_domain&gt;&lt;sys_domain_path&gt;/&lt;/sys_domain_path&gt;&lt;sys_id&gt;8419adfe372023001d52ded993990e8c&lt;/sys_id&gt;&lt;sys_mod_count&gt;1&lt;/sys_mod_count&gt;&lt;sys_name&gt;Insert with Overrides&lt;/sys_name&gt;&lt;sys_overrides/&gt;&lt;sys_package display_value="Global" source="global"&gt;global&lt;/sys_package&gt;&lt;sys_policy/&gt;&lt;sys_scope display_value="Global"&gt;global&lt;/sys_scope&gt;&lt;sys_update_name&gt;sys_ui_action_8419adfe372023001d52ded993990e8c&lt;/sys_update_name&gt;&lt;sys_updated_by&gt;admin&lt;/sys_updated_by&gt;&lt;sys_updated_on&gt;2018-09-18 11:54:12&lt;/sys_updated_on&gt;&lt;table&gt;contact_definition&lt;/table&gt;&lt;ui11_compatible&gt;false&lt;/ui11_compatible&gt;&lt;ui16_compatible&gt;false&lt;/ui16_compatible&gt;&lt;/sys_ui_action&gt;&lt;/record_update&gt;</payload><payload_hash>812808934</payload_hash><remote_update_set display_value="MIM - Insert with Overrides UI Action">df93cc7bdba8e3405dbe38f0ad961973</remote_update_set><replace_on_upgrade>false</replace_on_upgrade><sys_created_by>brent.llewellyn</sys_created_by><sys_created_on>2018-09-21 07:00:32</sys_created_on><sys_id>d393cc7bdba8e3405dbe38f0ad961974</sys_id><sys_mod_count>0</sys_mod_count><sys_recorded_at>165f6f28fbe0000001</sys_recorded_at><sys_updated_by>brent.llewellyn</sys_updated_by><sys_updated_on>2018-09-21 07:00:32</sys_updated_on><table>contact_definition</table><target_name>Insert with Overrides</target_name><type>UI Action</type><update_domain>global</update_domain><update_guid>a70a6d32486023007b3f0ca85e75561b</update_guid><update_guid_history>a70